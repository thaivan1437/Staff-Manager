{
    "name": "company-web",
    "version": "1.0.0",
    "sasslintConfig": "/.sass-lint.yml",
    "scripts": {
        "local": "NODE_ENV=test PORT=3005 ts-node --project tsconfig.server.json server/index.ts",
        "build": "next build",
